{
  "name": "aispace2",
  "version": "0.5.0",
  "description": "A Jupyter extension for the next-generation of AISpace.",
  "author": "AISpace2",
  "license": "GPL-3.0",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aispace2/aispace2.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "dev": "npm run build:dev-dll && webpack-dashboard -m -- webpack --env=development --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier \"src/**/*.{ts,js,json,css}\" --write",
    "build:dev": "npm run build:dev-dll && webpack --env=development --progress",
    "build:dev-dll": "webpack --config webpack.dll.js",
    "build:prod": "webpack --env=production --progress"
  },
  "devDependencies": {
    "@types/d3": "^4.10.0",
    "@types/node": "^8.0.23",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "fork-ts-checker-webpack-plugin": "^0.2.7",
    "html-loader": "^0.5.1",
    "json-loader": "^0.5.7",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.1",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^0.4.0",
    "webpack-merge": "^4.1.0",
    "webpack-sources": "^1.0.1"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "d3": "^4.10.0",
    "jquery": "^3.2.1",
    "shortid": "^2.2.8",
    "underscore": "^1.8.3",
    "vue": "^2.4.2",
    "vue-class-component": "^5.0.2",
    "vue-property-decorator": "^5.1.1"
  }
}
