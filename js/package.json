{
  "name": "aispace2",
  "version": "0.8.4",
  "_comment_about_version_number": "Do not change this manually. Change this by runnning /updateVersion.py",
  "description": "A Jupyter extension for the next-generation of AISpace.",
  "author": "AISpace2",
  "license": "GPL-3.0",
  "main": "src/index.js",
  "jlab_extension_id": "aispace2",
  "repository": {
    "type": "git",
    "url": "https://github.com/aispace2/aispace2.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "dev": "npm run build:dev-dll && webpack-dashboard -m -- webpack --env=development --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier \"src/**/*.{ts,js,json,css,vue}\" --write",
    "build:dev": "npm run build:dev-dll && webpack --env=development --progress",
    "build:dev-dll": "webpack --config webpack.dll.js",
    "build:prod": "webpack --env=production --progress",
    "update-lab-extension": "webpack --config webpack.labExtension.js"
  },
